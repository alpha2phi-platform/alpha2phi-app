
{
  "all": true,
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/*.tsx",
    "src/*.ts"
  ],
  "exclude": [
    "cypress/**/*.*",
    "src/__tests__/*.*",
    "src/test/*.*",
    "src/stories/*.*",
    "**/*.d.ts",
    "**/*.cy.tsx",
    "**/*.cy.ts"
  ]
}
